properties([[$class: 'JiraProjectProperty'], [$class: 'RebuildSettings', autoRebuild: false, rebuildDisabled: false], parameters([string(defaultValue: 'hello', description: '', name: 'param1', trim: false)])])

pp()


def pp() {
    def myparams = currentBuild.rawBuild.getAction(ParametersAction)
    def pMap = [:]
     for( p in myparams ) {
        pMap[p.name.toString()] = p.value.toString()
     }
     
     println pMap
}
